{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Node Inspector",
      "type": "node",
      "request": "launch",
      "args": ["${workspaceRoot}/src/index.ts"],
      "runtimeArgs": ["-r", "ts-node/register"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "NODE_ENV": "development"
      },
      "outputCapture": "std"
    }
  ]
}

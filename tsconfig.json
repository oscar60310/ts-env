{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "esnext",
    "paths": {
      "@models/*": [
        "models/*"
      ]
    },
    "baseUrl": "./src/",
    "typeRoots": [
      "./node_modules/@types/",
    ],
    "watch": true,
  },
  "include": [
    "./src/**/*"
  ]
}
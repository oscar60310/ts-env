{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "esnext",
    "paths": {
      "@models/*": ["models/*"]
    },
    "baseUrl": ".",
    "typeRoots": ["./node_modules/@types/"],
    "watch": true,
    "esModuleInterop": true
  },
  "include": ["./src/**/*"],
  "exclude": ["./src/**/*.spec.ts"]
}
